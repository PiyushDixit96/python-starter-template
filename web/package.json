{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "openapi": "pnpm dlx @hey-api/openapi-ts -i openapi.json -c @hey-api/client-fetch -o client",
    "typecheck": "react-router typegen && tsc",
    "typegen:watch": "react-router typegen --watch"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.1.1",
    "@clerk/clerk-react": "^5.15.3",
    "@epic-web/invariant": "^1.0.0",
    "@hey-api/client-fetch": "^0.4.3",
    "@react-router/node": "7.0.0-pre.5",
    "@react-router/serve": "7.0.0-pre.5",
    "@sentry/react": "^8.37.1",
    "client": "link:./client",
    "isbot": "^5.1.17",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "posthog-js": "^1.184.0",
    "ramda": "^0.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router": "7.0.0-pre.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@hey-api/openapi-ts": "^0.55.2",
    "@react-router/dev": "7.0.0-pre.5",
    "@sentry/vite-plugin": "^2.22.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/ramda": "^0.30.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": ">=8.14.0",
    "@typescript-eslint/parser": ">=8.14.0",
    "autoprefixer": "^10.4.20",
    "depcheck": "^1.4.7",
    "eslint": "^9.14.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "typesync": "^0.13.4",
    "vite": "^5.4.11",
    "vite-plugin-terminal": "^1.2.0",
    "vite-tsconfig-paths": "^5.1.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.12.3"
  },
  "prettier": {
    "semi": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrder": [
      "^~/configuration$",
      "./+types",
      "^[:alpha].*$",
      "^@remix(.*)$",
      "^(@mantine|@emotion|@nivo)(.*)$",
      "^@prisma(.*)$",
      "^~/",
      "./"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "ignorePath": [
      "*.lock"
    ]
  }
}
