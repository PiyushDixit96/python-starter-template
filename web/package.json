{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "pnpm run openapi && react-router typegen && safe-routes typegen && react-router build",
    "test": "pnpm run openapi && vitest run",
    "start": "react-router-serve ./build/server/index.js",
    "openapi": "pnpm dlx @hey-api/openapi-ts",
    "typecheck": "react-router typegen && tsc",
    "typegen:watch": "react-router typegen --watch"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.22.11",
    "@clerk/shared": "^2.20.18",
    "@epic-web/invariant": "^1.0.0",
    "@hey-api/client-fetch": "^0.8.1",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-router/dev": "7.1.5",
    "@react-router/node": "7.1.5",
    "@react-router/serve": "7.1.5",
    "@sentry/react": "^8.54.0",
    "@sentry/vite-plugin": "^3.1.2",
    "@tailwindcss/vite": "^4.0.4",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "client": "link:./client",
    "clsx": "^2.1.1",
    "isbot": "^5",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "lucide-react": "^0.474.0",
    "postcss": "^8.5.1",
    "posthog-js": "^1.215.5",
    "ramda": "^0.30.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-router": "7.1.5",
    "react-router-devtools": "2.0.0-beta.0",
    "safe-routes": "^1.0.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-terminal": "^1.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@hey-api/openapi-ts": "^0.64.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/ramda": "^0.30.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": ">=8.23.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "happy-dom": "^17.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": "^22.11.0",
    "pnpm": "^9.15.5"
  },
  "prettier": {
    "semi": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrder": [
      "^~/configuration$",
      "^[:alpha].*$",
      "\\+types",
      "^(~|@)/",
      "./"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  }
}
