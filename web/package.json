{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "pnpm run openapi && react-router build",
    "test": "pnpm run openapi && vitest run",
    "start": "react-router-serve ./build/server/index.js",
    "openapi": "pnpm dlx @hey-api/openapi-ts -i $OPENAPI_JSON_PATH -c @hey-api/client-fetch -o client",
    "typecheck": "react-router typegen && tsc",
    "typegen:watch": "react-router typegen --watch"
  },
  "dependencies": {
    "@clerk/clerk-js": "^5.37.0",
    "@clerk/clerk-react": "^5.18.0",
    "@epic-web/invariant": "^1.0.0",
    "@hey-api/client-fetch": "^0.5.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-router/node": "7.0.2",
    "@react-router/serve": "7.0.2",
    "@sentry/react": "^8.42.0",
    "class-variance-authority": "^0.7.1",
    "client": "link:./client",
    "clsx": "^2.1.1",
    "isbot": "^5",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "lucide-react": "^0.464.0",
    "posthog-js": "^1.194.3",
    "ramda": "^0.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router": "7.0.2",
    "react-router-devtools": "2.0.0-beta.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@react-router/dev": "7.0.2",
    "@sentry/vite-plugin": "^2.22.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/ramda": "^0.30.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": ">=8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "happy-dom": "^15.11.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.2",
    "vite-plugin-terminal": "^1.2.0",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22.11.0",
    "pnpm": ">=9.14.4"
  },
  "prettier": {
    "semi": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrder": [
      "^~/configuration$",
      "./+types",
      "^[:alpha].*$",
      "^~/",
      "./"
    ],
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
