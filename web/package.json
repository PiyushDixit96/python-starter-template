    "clsx": "^2.1.1",
    "isbot": "^5",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "lucide-react": "^0.510.0",
    "postcss": "^8.5.3",
    "posthog-js": "^1.242.1",
    "ramda": "^0.30.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
